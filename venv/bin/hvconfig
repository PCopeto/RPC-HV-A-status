#!/home/rpcuser/caen_HV/RPC-HV-A-status/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pycaenhv==0.0.2','console_scripts','hvconfig'
__requires__ = 'pycaenhv==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pycaenhv==0.0.2', 'console_scripts', 'hvconfig')()
    )
